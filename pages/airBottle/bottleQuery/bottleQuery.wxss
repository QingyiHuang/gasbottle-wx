/* pages/airBottle/bottleQuery/bottleQuery.wxss */
.container{
	height: 100vh;
	box-sizing: border-box;
  overflow: hidden;
}
.headerBox{
  background-color: #ffffff;
  display: grid;
  grid-template-columns: calc(100% - 96rpx) 96rpx;
}
.sandiv{
  width: 48rpx;
  height: 48rpx;
  position: relative;
  top: 32rpx;
  left: 4rpx;
}
.scandivimage{
  width: 100%;
  height: 100%;
}
.scrollBox{
  background-color: rgb(245, 245, 245);
  height: calc(100% - 216rpx);
}
.list-item{
	border-radius: 16rpx;
	/* bg/bg-primary */
	background: rgb(255, 255, 255);
	margin: 8rpx 16rpx;
	display: block;
	margin-bottom: 20rpx;
	position: relative;
	padding: 24rpx;
}
.list-item-tag{
  padding: 4rpx 16rpx;
  color: #ffffff;
	position: absolute;
	right: 24rpx;
  top: 24rpx;
  border-radius: 4rpx;
  background-color: rgb(29, 184, 76);
  color: rgb(255, 255, 255);
  font-size: 28rpx;
  font-weight: 400;
}
.tag1{
  background-color: rgb(29, 184, 76);
}
.tag2{
  background-color: rgb(230, 32, 42);
}
.tag22{
  background-color: rgb(247, 143, 25);
}
.tag3{
  background-color: rgb(77, 77, 77);
}
.tag4{
  background-color: rgb(27, 184, 247);
}
.list-item-title{
	color: rgba(0, 0, 0, 0.85);
	font-size: 32rpx;
	font-weight: 550;
	line-height: 48rpx;
	letter-spacing: 0px;
	display: block;
	text-align: left;
  white-space: nowrap;
  overflow: hidden;
	text-overflow: ellipsis;
	padding-right: 150rpx;
	margin-bottom: 16rpx;
}
.oneInfo{
	position: relative;
}
.list-item-left{
	width: 150rpx;
	color: rgba(0, 0, 0, 0.4);
	font-size: 28rpx;
	font-weight: 400;
	line-height: 36rpx;
	letter-spacing: 0px;
	text-align: left;
	position: absolute;
	left: 0;
	top: 0;
	word-break: keep-all;
}
.list-item-right{
	color: rgb(51, 51, 51);
	font-size: 28rpx;
	font-weight: 400;
	line-height: 40rpx;
	letter-spacing: 0px;
	text-align: left;
	margin-left: 150rpx;
}
.loading-more {  
	text-align: center;  
	margin-top: 40rpx;
}  
	
.no-more-data {  
	text-align: center;  
	margin-top: 40rpx;
}