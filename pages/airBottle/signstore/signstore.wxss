.container {
  position: relative;
  height: 100vh;
}
.scanner {
  width: 100%;
  height: 60%;
}
.overlay {
  position: absolute;
  box-sizing: border-box;
  overflow: hidden;
  width: 100%;
  height: 50%;
  bottom: 0;
  left: 0;
  background-color: rgb(245, 246, 247);
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
}
.overlay2{
  position: absolute;
  box-sizing: border-box;
  overflow: hidden;
  width: 100%;
  height: 95%;
  bottom: 0;
  left: 0;
  background-color: rgb(245, 246, 247);
  border-top-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
}
.rotate180{
  transform: rotate(180deg);
}
.info {
  margin-bottom: 40rpx;
}
.coverTitlePart{
  height: 96rpx;
  position: relative;
}
.coverTitleLeft{
  color: rgb(0, 0, 0);
  font-size: 32rpx;
  font-weight: 550;
  line-height: 96rpx;
  letter-spacing: 0px;
  text-align: left;
  position: absolute;
  left: 32rpx;
  top: 0;
}
.coverTitleRight{
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  right: 48rpx;
  top: 30rpx;
  color: rgb(0, 0, 0);
  font-size: 32rpx;
  font-weight: 500;
}
.coverTitleRightImage{
  width: 36rpx;
  height: 22rpx;
  transition: all 0.2s ease;
}
.footerPart{
  position: absolute;
  left: 0;
  bottom: 0;
  box-shadow: 0rpx -8rpx 18rpx 0rpx rgba(0, 0, 0, 0.08);
  backdrop-filter: blur(21.74rpx);
  background: rgba(251, 251, 251, 0.8);
  height: 128rpx;
  width: 100%;
}
.footerBtnOuterLeft {
  position: absolute;
  left: 0;
  top: 0;
  width: 50%;
  height: 100%;
  padding: 22rpx 0 0 20rpx;
}
.footerBtnOuterRight{
  position: absolute;
  right: 0;
  top: 0;
  width: 50%;
  height: 100%;
  padding: 22rpx 20rpx 0 0;
}
.footerBtn{
  width: calc(100% - 60rpx);
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin-top: 16rpx;
}
.coverDetail{
  height: calc(100% - 226rpx);
  /* overflow-y: scroll; */
  position: relative;
  padding: 0 32rpx;
}
.emptyPart{
  position: relative;
  width: 280rpx;
  text-align: center;
  padding-bottom: 48rpx;
  top: 80rpx;
  left: 50%;
  margin-left: -140rpx;
}
.emptyPartText{
  position: absolute;
  left: 0;
  bottom: 0;
  color: rgb(153, 153, 153);
  font-size: 28rpx;
  font-weight: 400;
  letter-spacing: 0px;
  text-align: center;
}
.emptyimg{
  width: 160rpx;
  height: 160rpx;
  display: block;
  margin: 0 auto;
}
.plainBtn{
  border-radius: 60rpx !important;
  border: 2rpx solid rgb(32, 128, 247) !important;
  color: rgb(32, 128, 247) !important;
}
.plainBtnDisabled{
  border-radius: 60rpx !important;
  border: 2rpx solid rgb(168, 204, 249) !important;
  color: rgb(168, 204, 249) !important;
}
.blueBtn{
  background: rgb(61, 117, 244) !important;
  border-radius: 60rpx !important;
}
.grayBtn{
  background: rgba(111, 152, 248, 0.671) !important;
  border-radius: 60rpx !important;
}
.fullOver{
  height: 95%;
}
.oneGas{
  height: 96rpx;
  /* width: calc(100% - 80rpx); */
  border-radius: 16rpx;
  background: rgb(255, 255, 255);
  position: relative;
  margin-bottom: 16rpx;
  color: rgb(51, 51, 51);
  font-size: 32rpx;
  font-weight: 500;
  line-height: 96rpx;
  /* padding-left: 80rpx; */
}
.gasIcon{
  width: 22rpx;
  height: 32rpx;
  position: absolute;
  top: 32rpx;
  left: 32rpx;
}
.closeIcon{
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 24rpx;
  right: 32rpx;
}
.outstoreTag{
  border-radius: 8rpx;
  background: rgb(197, 244, 205);
  color: rgb(29, 184, 76);
  font-size: 24rpx;
  font-weight: 400;
  letter-spacing: 0px;
  padding: 4rpx 8rpx;
  position: absolute;
  right: 96rpx;
  top: 32rpx;
}
.gasList{
  height: 100%;
  width: 100%;
  overflow-x:hidden;
  overflow-y:scroll;
  box-sizing: border-box;
}
.coverMask{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.coverMaskText{
  color: rgb(255, 255, 255);
  font-size: 24rpx;
  font-weight: 600;
  letter-spacing: 0;
  text-align: center;
  position: absolute;
  width: 100%;
  top: 10%;
  left: 0;
}
.coverMaskImg{
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}