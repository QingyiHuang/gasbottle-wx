/* pages/citycase/tododetail/tododetail.wxss */
.imgback{
    width: 100%;
    height: 100%;
    
}
.topImgPart{
    height: 480rpx;
    overflow: hidden;
}
.detailContent{
    height: 100vh;
    position: relative;
}
.case-details-content {
    position: relative;
    padding: 150rpx 24rpx 24rpx 24rpx;
    background-color: rgba(245, 245, 245, 0.8);
    box-sizing: border-box;
    margin-top: 36rpx;
}
.box {
    border-radius: 16rpx 16rpx 0 0;
    height: 100%;
    background: #fff;
}

.box-info {
    height: calc(100% - 88rpx);
    overflow: auto;
}
.time-status {
    position: absolute;
    right: 0;
    top: 0;
    height: 48rpx;
    display: flex;
    align-items: center;
    font-size: 24rpx;
    text-align: center;
    font-weight: 400;
  }
  .overtime {
    color: #f55545;
  }
  .leave {
    color: #1989fa;
  }
  .tags {
    height: 40rpx;
    display: flex;
  }
  .tagdiv {
    height: 40rpx;
    line-height: 36rpx;
    padding: 0 24rpx;
    border: 2rpx solid transparent;
    border-radius: 20rpx;
    font-size: 24rpx;
    margin-right: 16rpx;
  }
    .level-box {
      background: rgba(255, 255, 255, 0.8);
      border-color: rgba(102, 102, 102, 0.5);
      color: #666666;
      padding-left: 16rpx;
    }
    .level-0 {
        background: #27d045;
    }
    .level-1 {
      background: #0089ff;
    }
    .level-2 {
      background: #ff952c;
    }
    .level-3 {
      background: #f55545;
    }

    .duban {
      background: rgba(33, 150, 243, 0.1);
      border-color: rgba(33, 150, 243, 0.5);
      color: #0089ff;
    }

    .urging {
      background: rgba(245, 85, 69, 0.1);
      border-color: rgba(245, 85, 69, 0.5);
      color: #f55545;
    }
    .main-info{
        position: absolute;
        top: -48rpx;
        left: 24rpx;
        right: 24rpx;
        height: 168rpx;
        padding: 24rpx;
        background: #fff;
        box-shadow: 0rpx 8rpx 32rpx -8rpx rgba(0, 0, 0, 0.2);
        border-radius: 16rpx;
        z-index: 1; 
        box-sizing: border-box;
    }
    .main-info-name{
        position: relative;
        height: 48rpx;
        padding-left: 48rpx;
        font-size: 32rpx;
        color: #333333;
        letter-spacing: 0;
        line-height: 48rpx;
        font-weight: 600;
        margin: 0 0 24rpx;
        background: url('data:image/png;base64,') 0 center no-repeat;
        background-size: 32rpx 32rpx;
    }
    .level-boxbefore{
        float: left;
        margin: 12rpx 10rpx 0 0;
        width: 16rpx;
        height: 16rpx;
        border-radius: 8rpx;
    }
    .box {
        background: #fff;
        box-shadow: 0rpx 8rpx 32rpx -8rpx rgba(0, 0, 0, 0.2);
        border-radius: 16rpx;
        z-index: 1; 
        box-sizing: border-box;
      }
  
      .box-info {
        height: calc(100% - 88rpx);
        overflow: auto;
      }
      .van-cell {
        font-size: 32rpx;
      }
      .van-cell .van-cell__title {
          color: #999999;
      }
      .van-cell .van-cell__title .is-required {
        position: relative;
      }
        .van-cell .van-cell__value {
          color: #333333;
        }
    
        .van-cell .left-value .van-cell__title {
            color: #999999;
            flex: 0 1 160rpx;
        }
        .van-cell .left-value .van-cell__value {
          text-align: left;
        }
      
      .van-form .van-cell__title {
        color: #333333;
      }
.process-list .empty-process {
    height: 352rpx;
    padding-top: 160rpx;
    text-align: center;
}
.stepsBox{
    padding: 24rpx 40rpx;
}
.oneStep{
    border-left: 2rpx solid rgb(215, 219, 228);
    padding-left: 24rpx;
    position: relative;
    padding-bottom: 4rpx;
}
.stepPoint{
    width: 36rpx;
    height: 36rpx;
    position: absolute;
    left: -20rpx;
    top: -6rpx;
}
.process-list .oneStep .time {
  color: rgb(51, 51, 51);
  font-size: 28rpx;
  font-weight: 400;
  position: relative;
  top: -12rpx;
}
.timePerson{
  font-size: 28rpx;
  font-weight: 400;
  line-height: 48rpx;
  letter-spacing: 0rpx;
  color: rgb(0, 168, 255);
  margin-right: 16rpx;
}
.timeLine{
  color: rgb(153, 153, 153);
  font-size: 24rpx;
  font-weight: 400;
  line-height: 34rpx;
  letter-spacing: 0rpx;
  position: relative;
  top: -4rpx;
}
.process-list .oneStep .info {
  line-height: 48rpx;
  word-break: break-all;
  color: rgb(51, 51, 51);
  font-size: 28rpx;
  font-weight: 400;
  line-height: 48rpx;
  letter-spacing: 0rpx;
}
.infoBox{
  border-radius: 8rpx;
  background-color: rgb(16, 142, 233, 0.08);
  padding: 16rpx;
  margin-bottom: 16rpx;
}
.handleBtn{
    height: 80rpx;
    background-color: #f55545;
    margin-bottom: 64rpx;
    margin-top: 24rpx;
    display: grid;
    grid-template-columns: 50% 50%;
}

.swiper-container {  
  width: 100%;
  height: 30vh; 
  position: relative;  
  overflow: hidden;  
}  
  
.slide-image {  
  width: 100%;  
  height: 100%;  
}
.media-box {
  width: 100%;
  height: 10vh;
}
.cuiban-tips {
    height: 40rpx;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 0;
    font-weight: 400;
    margin: 0 0 16rpx;
  }
  .field {
    background: #ebebeb;
    border-radius: 8rpx;
    padding: 8rpx 32rpx;
  }
  