/* pages/enterprise/patrol/patrol.wxss */
.container{
	height: 100vh;
	box-sizing: border-box;
	overflow: hidden;
}
.headerBox{
	width: 100%;
	background-color: #ffffff;
}
.list-item{
	border-radius: 16rpx;
	/* bg/bg-primary */
	background: rgb(255, 255, 255);
	margin: 8rpx 16rpx;
	display: block;
	margin-bottom: 20rpx;
	position: relative;
	padding: 24rpx;
}
.list-item-tag{
	width: 40rpx;
	height: 40rpx;
	border-radius: 50%;
	background-color: rgb(251, 206, 202);
	color: rgb(192, 20, 34);
	font-size: 24rpx;
	font-weight: 400;
	line-height: 40rpx;
	text-align: center;
	position: absolute;
	right: 24rpx;
	top: 24rpx;
}
.list-item-tag2{
	width: 40rpx;
	height: 40rpx;
	border-radius: 50%;
	background-color: rgb(198, 255, 187);
	color: rgb(18, 158, 0);
	font-size: 24rpx;
	font-weight: 400;
	line-height: 40rpx;
	text-align: center;
	position: absolute;
	right: 24rpx;
	top: 24rpx;
}
.list-item-title{
	color: rgba(0, 0, 0, 0.85);
	font-size: 32rpx;
	font-weight: 550;
	line-height: 48rpx;
	letter-spacing: 0rpx;
	display: block;
	text-align: left;
  white-space: nowrap;
  overflow: hidden;
	text-overflow: ellipsis;
	padding-right: 52rpx;
	margin-bottom: 16rpx;
}
.oneInfo{
	position: relative;
}
.list-item-left{
	width: 150rpx;
	color: rgba(0, 0, 0, 0.4);
	font-size: 28rpx;
	font-weight: 400;
	line-height: 36rpx;
	letter-spacing: 0rpx;
	text-align: left;
	position: absolute;
	left: 0;
	top: 0;
	word-break: keep-all;
}
.list-item-right{
	color: rgb(51, 51, 51);
	font-size: 28rpx;
	font-weight: 400;
	line-height: 40rpx;
	letter-spacing: 0rpx;
	text-align: left;
	margin-left: 150rpx;
}
.boxinner{
	background: rgb(245, 245, 245);
	height: calc(100vh - 232rpx);
	padding: 16rpx 0;
}
.scrollBox{
  height: calc(100vh - 200rpx); 
  box-sizing: border-box; 
  padding-top: 16rpx;
  background-color: rgb(245, 246, 247);
}
.loading-more {  
  text-align: center;  
  margin-top: 40rpx;  
  /* 其他样式 */  
}  
  
.no-more-data {  
  text-align: center;  
  margin-top: 40rpx;  
  /* 其他样式 */  
}

.search-filter {
  display: flex;
  align-items: center;
  width: 100%;
  padding: 16rpx 0;
}

.search-filter .van-dropdown-menu {
  width: 160rpx;
  height: 100%;
  margin-right: 16rpx;
}

.search-filter .van-dropdown-menu__item {
  height: 80rpx;
  line-height: 80rpx;
}

.search-filter .van-dropdown-menu__title {
  font-size: 28rpx;
}