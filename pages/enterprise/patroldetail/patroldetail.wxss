	font-weight: 500;
	line-height: 60rpx;
	letter-spacing: 0rpx;
	text-align: left;
	padding-right: 160rpx;
	word-break: break-all;
	margin-bottom: 36rpx;
}
.tagImg{
	position: absolute;
	width: 192rpx;
	height: 192rpx;
	right: -20rpx;
	top: -20rpx;
}
.tagImgInner{
	width: 100%;
	height: 100%;
}
.oneInfo{
	position: relative;
	margin-bottom: 16rpx;
	min-height: 28rpx;
}
.oneInfo-left{
	color: rgba(0, 0, 0, 0.4);
	font-size: 28rpx;
	font-weight: 400;
	letter-spacing: 0rpx;
